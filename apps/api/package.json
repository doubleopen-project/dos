        "cors": "2.8.5",
        "database": "*",
        "express": "4.18.2",
        "express-session": "1.17.3",
        "is-glob": "4.0.3",
        "minimatch": "9.0.3",
        "node-cache": "5.1.2",
        "node-cron": "3.0.3",
        "packageurl-js": "1.2.1",
        "passport": "0.7.0",
