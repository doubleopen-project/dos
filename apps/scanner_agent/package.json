{
    "name": "scanner-agent",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "build": "tsup --format cjs",
        "clever-logs": "clever logs",
        "dev": "tsup src/server.ts --format cjs --watch --onSuccess \"node --env-file ../../.env.local dist/server.js\"",
        "lint": "tsc --noEmit && eslint \"src/**/*.ts*\"",
        "start": "node --env-file ../../.env.local dist/src/server.js",
        "test": "mocha dist/tests --exit --timeout 10000"
    },
    "dependencies": {
        "@zodios/express": "10.6.0",
        "@zodios/openapi": "10.5.0",
        "body-parser": "1.20.2",
        "bull": "4.12.2",
        "compression": "1.7.4",
        "cross-fetch": "4.0.0",
        "express": "4.18.2",
        "milliseconds": "1.0.3",
        "s3-helpers": "*",
        "swagger-ui-express": "5.0.0",
        "validation-helpers": "*"
    },
    "devDependencies": {
        "@types/chai": "4.3.14",
        "@types/compression": "1.7.5",
        "@types/express": "4.17.21",
        "@types/milliseconds": "0.0.33",
        "@types/mocha": "10.0.6",
        "@types/node": "20.12.7",
        "@types/swagger-ui-express": "4.1.6",
        "chai": "5.1.0",
        "chai-http": "4.4.0",
        "eslint-config-custom-server": "*",
        "mocha": "10.3.0",
        "nock": "13.5.3",
        "tsconfig": "*",
        "tsup": "8.0.2",
        "typescript": "5.4.5"
    }
}
