{
    "name": "scanner-agent",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "build": "tsup src/server.ts --format cjs",
        "build:worker": "tsup src/worker.ts --format cjs",
        "dev": "tsup src/server.ts --format cjs --watch --onSuccess \"node dist/server.js\"",
        "lint": "tsc --noEmit && eslint \"src/**/*.ts*\"",
        "start": "node dist/server.js",
        "start:worker": "node dist/worker.js",
        "worker": "tsup src/worker.ts --format cjs --watch --onSuccess \"node dist/worker.js\""
    },
    "dependencies": {
        "@types/throng": "^5.0.4",
        "bull": "^4.10.4",
        "express": "^4.18.2",
        "throng": "^5.0.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.17",
        "@types/node": "^18.15.11",
        "eslint": "^8.38.0",
        "eslint-config-custom-server": "*",
        "tsconfig": "*",
        "tsup": "^6.7.0",
        "typescript": "^5.0.3"
    }
}
