{
    "name": "scanner-agent",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "build": "tsup --format cjs",
        "dev": "tsup src/server.ts --format cjs --watch --onSuccess \"node dist/server.js\"",
        "lint": "tsc --noEmit && eslint \"src/**/*.ts*\"",
        "start": "node dist/src/server.js",
        "worker": "tsup src/worker.ts --format cjs --legacy-output --watch --onSuccess \"node dist/worker.js\"",
        "test": "mocha dist/tests --exit --timeout 10000"
    },
    "dependencies": {
        "@zodios/express": "10.6.0",
        "@zodios/openapi": "10.5.0",
        "body-parser": "1.20.2",
        "bull": "4.10.4",
        "common-helpers": "*",
        "cross-fetch": "4.0.0",
        "express": "4.18.2",
        "milliseconds": "1.0.3",
        "s3-helpers": "*",
        "swagger-ui-express": "5.0.0",
        "throng": "5.0.0",
        "validation-helpers": "*"
    },
    "devDependencies": {
        "@types/chai": "4.3.5",
        "@types/express": "4.17.17",
        "@types/milliseconds": "0.0.30",
        "@types/mocha": "10.0.1",
        "@types/node": "20.4.5",
        "@types/swagger-ui-express": "4.1.3",
        "@types/throng": "5.0.4",
        "chai": "4.3.7",
        "chai-http": "4.4.0",
        "eslint": "8.46.0",
        "eslint-config-custom-server": "*",
        "mocha": "10.2.0",
        "nock": "13.3.2",
        "tsconfig": "*",
        "tsup": "6.7.0",
        "typescript": "5.0.2"
    }
}
