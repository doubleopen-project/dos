{
    "name": "s3-helpers",
    "main": "./dist/src/index.js",
    "module": "./dist/src/index.mjs",
    "types": "./index.d.ts",
    "scripts": {
        "build": "tsup && prettier --write dist/src/index.d.ts && (head -n 4 src/index.ts && cat dist/src/index.d.ts) > temp_file && mv temp_file index.d.ts",
        "test": "mocha dist/tests"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "3.468.0",
        "@aws-sdk/s3-request-presigner": "3.468.0",
        "stream": "0.0.2"
    },
    "devDependencies": {
        "@types/chai": "4.3.11",
        "@types/mocha": "10.0.6",
        "@types/node": "20.10.4",
        "chai": "4.3.10",
        "mocha": "10.2.0",
        "tsconfig": "*",
        "tsup": "8.0.1",
        "typescript": "5.3.3"
    }
}
